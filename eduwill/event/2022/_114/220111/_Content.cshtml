        TweenMax.fromTo($(".light-5"), 3.5, { rotation: -15 }, { rotation: 15, repeat: -1, yoyo: true, transformOrigin: "center bottom", perspective: 400, ease: Power4.eaesOut });
        TweenMax.fromTo($(".light-6"), 3.5, { rotation: 15 }, { rotation: -15, repeat: -1, yoyo: true, transformOrigin: "center bottom", perspective: 400, ease: Power4.eaesOut });
        TweenMax.fromTo($(".light-7"), 3.5, { rotation: -15 }, { rotation: 15, repeat: -1, yoyo: true, transformOrigin: "center bottom", perspective: 400, ease: Power4.eaesOut });
