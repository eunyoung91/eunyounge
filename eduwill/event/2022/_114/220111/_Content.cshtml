        TweenMax.fromTo($(".blink-new"), 0.35, { scale: 0.5, opacity: 0 }, { scale: 1, opacity: 1, delay: 0.5, repeat: -1, yoyo: true });
    }


    function slideEvent(){
		$('.teacher-slide').slick({
			autoplay: true,
			autoplaySpeed: 3000,
			speed: 1000,
			infinite: true,
			arrows:false,
			dots: true,
