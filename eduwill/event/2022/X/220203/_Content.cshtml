<script type="text/javascript">
    var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
    var url = window.location.href;
    var str = 'event.eduwill.net';
    var checkUrl = url.indexOf(str);
    $(function () {
        frontInit();
    });
    function frontInit() {
        getTimer('#promotionTimer', 'May 10,2021,23:59');
        checkResolution('.promotion-sale');
        pageMove('.page-move');
        addSlide();
		titleEvent();
    };
	  function titleEvent(){
        TweenMax.fromTo($('.sector-title .sale'), 1, { y: 0 }, { y: -10, repeat: -1, yoyo: true, ease: Bounce.eaesInOut });
    }
    function checkResolution(e) {
        if (isMobile || !checkUrl) {
            replaceImageLinks(e, 'pc', 'mobile');
            //console.log('mobile');
        } else {
            replaceImageLinks(e, 'mobile', 'pc');
            //console.log('pc');
        }
    }
    function replaceImageLinks(target, basic, change) {
        $(target + ' a').each(function () {
            var pcLink = $(this).data("pcLink");
            var mobileLink = $(this).data("mobileLink");
            $(this).attr("href", change == "pc" ? pcLink : mobileLink);
        });
    }
    function addSlide() {
        $('.slider-multiple').each(function () {
            if (isMobile) {
                _show = 1;
                _center = true;
            } else {
                _show = 2;
                _center = false;
            }
            $(this).slick({
                autoplay: true,
                autoplaySpeed: 3000,
                speed: 1000,
                infinite: true,
                arrows: false,
                dots: true,
                mobileFirst: true,
                centerMode: _center,
                slidesToShow: _show,
                slidesToScroll: 1
            });
        });
        if (isMobile) {
            $('.teacher-list').slick({
                autoplay: true,
                autoplaySpeed: 3000,
                speed: 1000,
                infinite: true,
                arrows: true,
                dots: false,
                mobileFirst: true,
                slidesToScroll: 1
            });
        }
    }
</script>