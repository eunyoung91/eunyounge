	function checkResolution(e){
		if( isMobile || !checkUrl ){
			replaceImageLinks(e, 'pc', 'mobile');
			//console.log('mobile');
		} else {
			replaceImageLinks(e, 'mobile', 'pc');
			//console.log('pc');
		}
	}

	function replaceImageLinks(target, basic, change) {
		$(target + ' a').each(function(){
			var pcLink = $(this).data('pcLink');
			var mobileLink = $(this).data('mobileLink');
			$(this).attr('href', change == 'pc' ? pcLink : mobileLink);
		});
	}


	
	$(window).on('scroll', function(){
		
		var wt = $(window).scrollTop();
		var section = $('.container');

		section.each(function(i){
			var _this = $(this);

			if( wt >= section.eq(i).offset().top - 600){
				_this.addClass('active');

				function _f_changeFix(num){
					var _fixWrap = $('#moveBanner');

					_fixWrap.find('li').removeClass('active');
					_fixWrap.find('li:nth-child('+num+')').addClass('active');
				}

				if( _this.hasClass('promotion-title')){
					_f_changeFix(1);

				}else if( _this.hasClass('promotion-cont3')){
					_f_changeFix(2);
				}
				else if( _this.hasClass('promotion-cont4')){
					_f_changeFix(3);

				}else if( _this.hasClass('promotion-cont5')){
					_f_changeFix(4);

				}else if( _this.hasClass('promotion-somun')){
					_f_changeFix(5);

				}else if( _this.hasClass('promotion-cont6')){
					_f_changeFix(0);
				}

			} else {
				_this.removeClass('active');
			}
		});
	});
	
