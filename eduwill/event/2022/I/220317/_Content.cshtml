	function _f_initclinicButton(){
		
		_f_initReasonButton();

		var _tabWrap = $('#tabWrap');
		var _tabButton = _tabWrap.find('li');
		var _contents = $('.clinicWrap').find('ul');

		_contents.each(function(i){
			var _indexPostBox = i;

			$(this).find('button').on('click', function(){
				
				_f_initClinic(_indexPostBox, $(this));
			});

		});

		_tabButton.each(function(){

			var _this = $(this);

			_this.on('click', function(){

				if( !_this.hasClass('active') ){
					
					_tabButton.removeClass('active');
					_this.addClass('active');
					_contents.removeClass('active');
					_contents.eq(_this.index()).addClass('active');
				}

			})

		});
	}

