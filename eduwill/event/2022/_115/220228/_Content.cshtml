<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.2/TweenMax.min.js"></script>

<script>

    var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
    var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);

	<!-- 0214 로드 시 비디오재생 -->
	if("@(ViewBag.platformCd)" == "W") {
		$('html').css('overflow-y', 'hidden');
		function closeIntro() {
			$('html').css('overflow-y', 'auto');
			$('.sector-intro').fadeOut();
			$('.sector-intro').empty();
			onComplete();
		}
		<!-- skip 누를 시 -->
		$('.sector-intro .btn-skip').on('click', function() {
			closeIntro();
		});
		<!-- 닫기 누를 시 -->
		$('.sector-intro .intro-close').on('click', function() {
			closeIntro();
		});
		<!-- mute효과 -->
		$(".sector-intro .btn-mute").on('click', function () {
			if ($("video").prop('muted')) {
				$("video").prop('muted', false);
				$(this).addClass('on');
			} else {
				$("video").prop('muted', true);
				$(this).removeClass('on');
			}
		});
		$('#teaserMV').on('ended', function() {
			closeIntro();
		});
	}
	if("@(ViewBag.platformCd)" == "M") {
		onComplete();
	<!-- 0228 영상관련 스크립트 수정 -->
	
		edufLayerVideoOpen('//pmp.eduwill.net/eduwillpmp/eduwill/flv/220228/teaser.mp4','autoplay muted controls');
		$(document).on('ready', function() {

			$(document).find('#edufLayerVideo video').on('ended', function() {
				$('#edufLayerVideo').remove();
			});
			
		});
	}
	

    init();

    function init() {
        slide();
		videoEvent();
    }


    function slide() {
        $('.slider').each(function() {
				if(isMobile){
					var arrow = false;
					var dots = true;
				}else{
					var arrow = true;
					var dots = false;
			}
			<!-- 0225 slick mo pc 구분짓기 -->
			if(isMobile) {
				$('.slider').slick({
					infinite:true,
					autoplay:true,
					autoplaySpeed: 3000,
					arrows:arrow,
					dots:dots,
					centerMode: true,
					slidesToShow: 1,
					slidesToScroll: 1,
				});
			} else {
				$('.slider').slick({
					infinite:true,
					autoplay:true,
					autoplaySpeed: 3000,
					arrows:arrow,
					dots:dots,
					centerMode: true,
					slidesToShow: 3,
					slidesToScroll: 1,
				});
			}
        })
    }

	<!-- 0228 앵커이동 부드럽게 -->
	pageMove('.page-move' , -48);

	<!-- 스크롤 시 하단 고정 -->
	var visualHeight = $('.sector-visual').innerHeight();
	var soonHeight = $('.sector-soon').innerHeight();
	$(window).scroll(function() {
		var scroll = $(this).scrollTop();
		if((visualHeight + soonHeight) <= scroll) {
			$('.sector-floating').show();
		} else {
			$('.sector-floating').hide();
		}
	})

	<!-- 하단 마우스오버 -->
	$('.sector-floating li').on({
		mouseenter: function() {
			$('.sector-floating li').removeClass('on');
			$('.sector-floating li').addClass('not');
			$(this).addClass('on');
		},
		mouseleave: function() {
			$('.sector-floating li.not').removeClass('not');
			$('.sector-floating li.on').removeClass('on');
		}
	});

	<!-- 0214 TweenMax -->
	function onComplete() {
		<!-- TweenMax -->
		var visualTarget = $('.sector-visual .wrap');
		var visualLaser = $('.sector-visual .laser-wrap');
		var visualBuilding = $('.sector-visual .building-wrap');
		var visualSpotlight = $('.sector-visual .spotlight');

		var visualMotion = new TimelineMax({ delay: 0, repeat: 0 });

		<!-- Title -->
		TweenMax.set(visualTarget.find('.title01'), { delay: 0, y: 100, opacity: 0})
		TweenMax.set(visualTarget.find('.title02'), { delay: 0, y: 100, opacity: 0})
		TweenMax.set(visualTarget.find('.title03'), { delay: 0, y: 100, opacity: 0})

		<!-- Spotlight -->
		TweenMax.set(visualSpotlight.find('img'), {delay: 0, opacity: 0});

		<!-- Building -->
		TweenMax.set(visualBuilding.find('.building'), { delay: 0, opacity: 0})

		<!-- Building Effect -->
		TweenMax.set(visualBuilding.find('.motion'), { delay: 0, opacity: 0})

		<!-- Title Tween -->
		<!-- 0225 트윈맥스 수정 -->
		if(isMobile) {
			TweenMax.to(visualTarget.find('.title01'), 1, { delay: 0.75, y: 0, opacity: 1, transformOrigin: 'center center'})
			TweenMax.to(visualTarget.find('.title02'), 1, { delay: 0.5, y: -6, opacity: 1, transformOrigin: 'center center'})
			TweenMax.to(visualTarget.find('.title03'), 1, { delay: 1, y: 0, opacity: 1, transformOrigin: 'center center'})
		}else {
			TweenMax.to(visualTarget.find('.title01'), 1, { delay: 0.5, y: 0, opacity: 1, transformOrigin: 'center center'})
			TweenMax.to(visualTarget.find('.title02'), 1, { delay: 0.75, y: -6, opacity: 1, transformOrigin: 'center center'})
