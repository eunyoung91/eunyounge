<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.2/TweenMax.min.js"></script>

<script>

    var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
    var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);

    init();

	var _w;
	var _breakpoint = 720;
	var _wid;
	var _hei;

	var _appList;
	var _appView;
	var _appPaging;

    function init() {
		create();
		addEvent();
    }

	function create() {
		_w = $(window);
		_wid = _w.width();
		_hei = _w.height();
	}
	
	$('.page-move').on('click', function(e) {
		e.preventDefault();
		$('html, body').animate({
			scrollTop: $(this.hash).offset().top
		}, 500);
		return false;
	});
<!-- 0318 menu 수정 -->
	$('.slider').each(function(idx, ele){
		var $this = $(this);
		$this.addClass('slider-0' + (idx + 1));

		var menu = [
			['AI 진단 테스트 / AI 중간 테스트', 'AI 진단 결과'],
			['오늘의 학습', '오늘의 공식 강의', '오늘의 공식 문제'],
			['토익 공식', '복습', '문제 모아 풀기', '한번에 풀기'],
			['최신 토익 기출 단어', '단어 가리기', '뜻 가리기']
		]

		var swiper = new Swiper(".slider-0" + (idx + 1) + " .swiper-container", {
			slidesPerView: 1,
			autoplay: {
				delay: 4000,
				autoplayDisableOnInteraction: false,
			},
			loop: true,
			navigation: {
				nextEl: ".slider-0" + (idx + 1) + " .swiper-button-next",
				prevEl: ".slider-0" + (idx + 1) + " .swiper-button-prev",
			},
			pagination: {
				el: '.slider-0' + (idx + 1) + ' .swiper-pagination',
				clickable: true,
				renderBullet: function (index, className) {
					return '<p class="' + className + '"">' + ((menu[idx])[index]) + '</p>';
				},
			},
		});
	});



	// eventAdd
	function addEvent() {
		_w.on('scroll',scrollEvent);
		scrollEvent();
	}
	

	// scrollEvent
	function scrollEvent() {
		var $scrollEl = $('.scroll-animate');
		var _sct = $(this).scrollTop();
		var _winHeight = $(this).height();
		$scrollEl.each(function (i) {
			var tg = $(this);
			var offset = tg.offset().top - _winHeight;

			if (offset < _sct) {
				$scrollEl.eq(i).addClass('active');
			} else {
				$scrollEl.eq(i).removeClass('active');
			}
		});
	}

	var acc = document.getElementsByClassName("faq-title");
	var i;

	for (i = 0; i < acc.length; i++) {
		acc[i].addEventListener("click", function () {
			this.classList.toggle("active");
			var panel = $(this).next();
			if ($(this).hasClass('active')) {
				$(panel).slideDown();
			} else {
				$(panel).slideUp();
			}
		});
	}


	
</script>