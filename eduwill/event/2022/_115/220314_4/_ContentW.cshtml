	// 리사이즈 이벤트
	function resizeEvent(){
		_wid =  $(window).width();
		_hei =  $(window).height();

		_section.css({height:_hei});

		// 푸터 높이 설정(고정높이)
		$footer.css({height:447});
		$(".sector-promotion").css({height:1691});

		// 모바일이 아니고 768보다 큰경우에만 마우스 힐실행
		showMouseWheel();

		// 리사이즈 발생시 딜레이함수 실행
		//(모바일기기에서 화면 떨림 현상이 있어서 웹에서만 실행)
		delayScrollEvent();
	
	}

	// 딜레이함수
	function delayScrollEvent(){
		if(_timer) {
			window.clearTimeout(_timer);
		}
		_timer = window.setTimeout(function() {
			// 스크롤의 위치에 따른 섹션위치 찾는 함수 
			showMoveSection(_currentSection);
		}, 600);
	}

	// 큰 섹션 이동(1,2,3)
	function showMoveSection(val) {
		_tween =  TweenMax.to($( "html, body" ), 0.5,{scrollTop:$("#section" + val).offset().top, ease:Power0.easeNone });
	}

	// 스크롤 이벤트 발생
	function scrollEvent(e){
		var sT = _w.scrollTop();

		setMotion();

		if(_currentSection == 1){
			$pagination.removeClass("show");
			$floating1.removeClass("show");
		}else if(_currentSection == 2){
			$pagination.removeClass("show");
			$floating1.addClass("show");
		}else{
			$pagination.addClass("show");
			$paginationLi.removeClass("active");
			$floating1.addClass("show");
			if(_currentSection < 3){
				$paginationLi.eq(0).addClass("active");
			}else if(_currentSection == 3){
				$paginationLi.eq(1).addClass("active");
			}else if(_currentSection == 4){
				$paginationLi.eq(2).addClass("active");
			}else if(_currentSection >= 5 && _currentSection < 7){
				$paginationLi.eq(3).addClass("active");
			}else if(_currentSection >= 7 && _currentSection < 9){
				$paginationLi.eq(4).addClass("active");
			}else if(_currentSection >= 9 && _currentSection < 11){
				$paginationLi.eq(5).addClass("active");
			}else if(_currentSection >= 11 && _currentSection < 13){
				$paginationLi.eq(6).addClass("active");
			}else if(_currentSection == 13){
				$paginationLi.eq(7).addClass("active");
			}else if(_currentSection >= 14 && _currentSection < 16){
				$paginationLi.eq(8).addClass("active");
			}else if(_currentSection >= 16){
				$paginationLi.eq(9).addClass("active");
			}
		}

		_section.each(function(index){           
			if(sT > $(this).offset().top-$(window).height()+$(window).height()/2){
				if(_isShow[index] == true){
					return;
				}
				_isShow[index] = true;
				_currentSection = index+1;

				if(_currentSection == 3){
					_isSection3Moving = true;
				}
			}else{
				if(_isShow[index] == false){
					return;
				}
				_isShow[index] = false;
				_currentSection = index;

				if(_currentSection == 3){
					_isSection3Moving = true;
				}
			}
		
		});

		if(_currentSection == 1){
			showSection1Motion();
		}else if(_currentSection == 2){
			showSection2Motion();
		}else if(_currentSection == 3){
			if(_isSection3Moving){
				showSection3Motion();
				_isSection3Moving = false;
			}
		}else if(_currentSection == 4){
			_prevSection4Index = 1;
			showSection4Motion(1);
		}else if(_currentSection == 5){
			showSection5Motion(_section5Index);
		}else if(_currentSection == 6){
			if(!_isSection6Moving){
				showSection6Motion();
				_isSection6Moving =true;
			} 
		}else if(_currentSection == 7){
		//	_isShowMoving = false;
			showSection7Motion();
		}else if(_currentSection == 8){
			showSection8Motion(_section8Index);
		}else if(_currentSection == 9){
			showSection9Motion();
		}else if(_currentSection == 10){
			if(!_isSection10Moving){
				showSection10Motion();
				_isSection10Moving =true;
			}
		}else if(_currentSection == 11){
			showSection11Motion();
		}else if(_currentSection == 12){
			if(!_isSection12Moving){
				showSection12Motion();
				_isSection12Moving =true;
			}
		}else if(_currentSection == 13){
		
			showSection13Motion(_section13Index);
		}else if(_currentSection == 14){
			showSection14Motion();
		}else if(_currentSection == 15){
			if(!_isSection15Moving){
				showSection15Motion();
				_isSection15Moving =true;
			}
		}

		// 스크롤 발생시 딜레이 함수 실행
		delayScrollEvent();
	}
</script>