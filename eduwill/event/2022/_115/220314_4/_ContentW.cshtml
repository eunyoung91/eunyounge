	// 섹션4 
	function showMoveSection4(val) {
		var current =  $(".section4-bg .bg"+val);
		var prev =  $(".section4-bg .bg"+_prevSection4Index);

		TweenMax.killTweensOf(current);
		TweenMax.killTweensOf(prev);
			
		if(_tween && _tween.progress() < 1){
			return;
		}

 		//$section5object.css({zIndex:1});

		// 내릴때
		if ( val > _prevSection4Index){    
			TweenMax.set( prev , { top:0,zIndex:2 });
			_tween = TweenMax.to( prev, 1, { top : "-100%", ease:Sine.easeInOut });
			
			TweenMax.set( current , { top:"100%",zIndex:3 });
			_tween = TweenMax.to( current, 0.7, { top : 0, ease:Sine.easeInOut });
		} else {
			// 올릴때
			TweenMax.set( prev , { top:0,zIndex:2 });
			_tween = TweenMax.to( prev, 1, { top : "100%",ease:Sine.easeInOut });
			TweenMax.set( current, { top:"-100%",zIndex:3 });
			_tween = TweenMax.to( current, 0.7, { top : 0, ease:Sine.easeInOut });
		} 

		// 실행중인 섹션 클래스 추가
		$('.section4-object').removeClass('active');
		$('.section4-obj'+val).addClass('active');

		showSection4Motion(val);   

		_prevSection4Index = val;    
	}


