	// 마우스 힐 이벤트
	function showMouseWheel(){   
		// 개별적으로 Wheel 이벤트 적용
		window.addEventListener('mousewheel', mousewheelEvent, {passive: false});
		window.addEventListener('DOMMouseScroll', mousewheelEvent, {passive: false});
		
	}

	 //20220311 수정
	function hideMouseWheel(){   
		window.removeEventListener('mousewheel', mousewheelEvent, {passive: false});
		window.removeEventListener('DOMMouseScroll', mousewheelEvent, {passive: false});
	}

	function mousewheelEvent(event){
		var sectionMax =_section.length;
		var section4Max = $section4object.length;
		var section8Max = $section8object.length;
		var section13Max = $section13object.length;

		if(_tween && _tween.progress() < 1){
			return;
		}

		// 마우스 힐 이벤트 발생시 모션 리셋
		setMotion();

		var delta = 0;
		if (!event) event = window.event;
		if (event.wheelDelta) {
			delta = event.wheelDelta / 120;
			if (window.opera) delta = -delta;
		} else if (event.detail) delta = -event.detail / 3;

		// 마우스휠을 위에서 아래로
		if (delta < 0) {
			if( _currentSection == 4 && _section4Index < section4Max){
					_section4Index++;
					showMoveSection4( _section4Index );
					
			}else if( _currentSection == 8 && _section8Index < section8Max){
					_section8Index++;
					showMoveSection8( _section8Index );
			}else if( _currentSection == 13 && _section13Index < section13Max){
					_section13Index++;
					showMoveSection13( _section13Index );
			} else {
				if(_currentSection < sectionMax){
					_currentSection++;
				
					showMoveSection(_currentSection);
				}
			}              

		// 마우스휠을 아래에서 위로
		} else {
			if( _currentSection == 4 && _section4Index > 1 ){
				_section4Index--;
				showMoveSection4( _section4Index );
			}else if( _currentSection == 8 && _section8Index > 1 ){
				_section8Index--;
				showMoveSection8( _section8Index );
			}else if( _currentSection == 13 && _section13Index > 1 ){
				_section13Index--;
				showMoveSection13( _section13Index );            
			} else {
				if(_currentSection > 1){
					_currentSection--;

					showMoveSection(_currentSection);
				}
			}
		}
	}



