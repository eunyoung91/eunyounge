<script src="//img.eduwill.net/eduwill/live/js/common/script.js"></script> *@
<script type="text/javascript">
	var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
	var agent = navigator.userAgent.toLowerCase();
	var trident = navigator.userAgent.match(/Trident\/(\d.\d)/i);
	var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);
	var _isProvideScroll = false;

	$(function(){
		init();
	});

	function init(){
		titleEvent();
		addEvent();
		scrollProvideEvent(null);
		$(window).on("scroll", scrollProvideEvent);
	};

	function addEvent(){
		pageMove('.page-move');
		/*if ("@(ViewBag.platformCd)" == "M") {
		}*/
	}
	
	function titleEvent(){
		var $title = $('.sector-title');

		setTimeout(function() { $(".title-1").css('opacity', '1').addClass('animate__fadeInDown animate__animated animate__faster') }, 100);
		setTimeout(function() { $(".title-2").css('opacity', '1').addClass('animate__swing animate__animated animate__faster') }, 300);
		setTimeout(function() { $(".title-3").css('opacity', '1').addClass('animate__fadeInDown animate__animated animate__faster') }, 400);

		setTimeout(function() { $(".btn-hint").css('opacity', '1').addClass('animate__swing animate__animated animate__infinite') }, 400);


		TweenMax.fromTo( $title.find('.flag'), .5, { opacity: 0, scaleY: 0}, { opacity: 1, scaleY: 1, delay: .5, ease: Power4.easeOut});
		TweenMax.fromTo( $title.find('.yut-1'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .5, ease: Bounce.easeOut});
		TweenMax.fromTo( $title.find('.yut-2'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .6, ease: Bounce.easeOut});
		TweenMax.fromTo( $title.find('.yut-3'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .7, ease: Bounce.easeOut});

		TweenMax.fromTo( $title.find('.tiger'), .4, { opacity: 0, scale: 0}, { opacity: 1, scale: 1, delay: .8, ease: Back.easeOut.config(3) });

		TweenMax.fromTo( $title.find('.light-1'), .5, { opacity: 0, scale: 0}, { opacity: 1, scale: 1, delay: .5, ease: Back.easeOut.config(1) });
		TweenMax.fromTo( $title.find('.light-2'), .5, { opacity: 0, scale: 0}, { opacity: 1, scale: 1, delay: .6, ease: Back.easeOut.config(1) });
		TweenMax.fromTo( $title.find('.light-3'), .5, { opacity: 0, scale: 0}, { opacity: 1, scale: 1, delay: .7, ease: Back.easeOut.config(1) });
	}

	function scrollProvideEvent(st) {
		var scrollTop = $(window).scrollTop();
		var offsetTop = $(".event-contents.step-2").offset().top;
		if (scrollTop > offsetTop - 500) {
			if (_isProvideScroll == true) return;
			TweenMax.fromTo( $('.event-contents').find('.yut-1'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .2, ease: Bounce.easeOut});
			TweenMax.fromTo( $('.event-contents').find('.yut-2'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .3, ease: Bounce.easeOut});
			TweenMax.fromTo( $('.event-contents').find('.yut-3'), .8, { opacity: 0, y: -300}, { opacity: 1, y: 0, delay: .4, ease: Bounce.easeOut});
			_isProvideScroll = true;
		} else {
			if (_isProvideScroll == false) return;
			TweenMax.fromTo( $('.event-contents').find('.yut-1'), .8, { opacity: 1}, { opacity: 0, delay: .1, ease: Power4.easeOut});
			TweenMax.fromTo( $('.event-contents').find('.yut-2'), .8, { opacity: 1}, { opacity: 0, delay: .1, ease: Power4.easeOut});
			TweenMax.fromTo( $('.event-contents').find('.yut-3'), .8, { opacity: 1}, { opacity: 0, delay: .1, ease: Power4.easeOut});
			_isProvideScroll = false;
		}
	}
</script>