@* <script src="//img.eduwill.net/eduwill/live/js/common/eduf.js"></script>
<script src="//img.eduwill.net/eduwill/live/js/common/script.js"></script> *@
<script type="text/javascript">
	var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
	var agent = navigator.userAgent.toLowerCase();
	var trident = navigator.userAgent.match(/Trident\/(\d.\d)/i);
	var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);

	$(function(){
		init();
	});

	function init(){
		addEvent();
		addSlide();
		winnerList();
	};

	function addEvent(){
		pageMove('.page-move');
		/*if ("@(ViewBag.platformCd)" == "M") {
		}*/

		var fileTarget = $('.file-box .upload-hidden');
		fileTarget.on('change', function(){
			if(window.FileReader){
				var filename = $(this)[0].files[0].name;
			} else {
				var filename = $(this).val().split('/').pop().split('\\').pop();
			}
			$(this).siblings('.upload-name').val(filename);
		});
	}
	
	function addSlide(){
		$('.review-slider').slick({
			autoplay: true,
			autoplaySpeed: 3000,
			speed: 800,
			infinite: true,
			arrows: false,
			dots: true,
			slidesToShow: 1,
			slidesToScroll: 1,
		});
	}

	function winnerList() {
		var options = {
			jsonFileNm: 'Q202012_WINNER.min',
		};

		var _row;
		var _show;
		if (isMobile && checkUrl != -1) {
			_row = 3;
			_show = 6;
		} else {
			_row = 6;
			_show = 10;
		}
		var slickOptions = {
			speed: 1000,
			slidesPerRow: _row,
			slidesToShow: _show,
		};

		eduf.UI.Winner('.winner-list', options, slickOptions);
	}
</script>