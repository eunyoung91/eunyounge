<script type="text/javascript">
	var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
	var agent = navigator.userAgent.toLowerCase();
	var trident = navigator.userAgent.match(/Trident\/(\d.\d)/i);
	var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);

	$(function(){
		init();
	});

	function init(){
		titleEvent();
		addEvent();
	};

	function addEvent(){
		pageMove('.page-move');
		/*if ("@(ViewBag.platformCd)" == "M") {
		}*/
	}
	
	function titleEvent(){
		var $title = $('.sector-title');
		_titleMotion();

		setTimeout(function() { $(".title-1").css('opacity', '1').addClass('animate__flipInX animate__animated animate__faster') }, 200);
		setTimeout(function() { $(".title-2").css('opacity', '1').addClass('animate__flipInX animate__animated') }, 400);
		setTimeout(function() { $(".teacher-1").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 500);
		setTimeout(function() { $(".name-1").css('opacity', '1').addClass('animate__fadeInUp animate__animated') }, 600);
		setTimeout(function() { $(".teacher-2").css('opacity', '1').addClass('animate__fadeInRight animate__animated animate__faster') }, 800);
		setTimeout(function() { $(".name-2").css('opacity', '1').addClass('animate__fadeInUp animate__animated') }, 900);

		function _titleMotion(){
			TweenMax.fromTo( $title.find('.light-1'), .5, { opacity: 0, scale: 0}, { opacity: 1, scale: 1, delay: .6, ease: Back.easeInOut.config(2), onComplete:function(){
				TweenMax.fromTo( $title.find('.light-1'), 1.5, { rotation:0}, { rotation: 360, delay: .1, repeat: -1, ease: Expo.easeInOut});
			}});
		}
	}
</script>