<script type="text/javascript">
    var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
    var url = window.location.href;
    var str = 'event.eduwill.net';
    var checkUrl = url.indexOf(str);

    $(function(){
        frontInit();
    });

    function frontInit(){
        pageMove('.page-move');
        scrollEvent();
        slideEvent();
        titleEvent();
        $(window).on('load', function(){
            $('.sector-banner .online-banner .bnrtitle').html('합격자 93%<sup>*</sup>가 선택한 수강패스');
            $('.sector-banner .online-banner').append('<p class="base">* 9급 수기를 작성한 에듀윌 최종합격자 중 평생패스 수강 비율(2019 기준)</p>');
        });
    }

    function titleEvent(){
        var promotionTitle = $('.sector-title');

        TweenMax.fromTo( promotionTitle.find('.title-1'), .6, { opacity: 0, y: -50, }, { opacity: 1, y: 0, delay: .2, ease: Power2.easeOut });
        TweenMax.fromTo( promotionTitle.find('.title-2'), .5, { opacity: 0, scale: 0, }, { opacity: 1, scale: 1, delay: .4, ease: Back.easeOut.config(2), onComplete: function(){
            TweenMax.fromTo( promotionTitle.find('.light'), .5, { opacity: 0, scale: 1.1 }, { opacity: 1, scale: 1, delay: .1, ease: Power2.easeOut, onComplete: function(){
                TweenMax.fromTo( promotionTitle.find('.light'), 2, { opacity: 1 }, { opacity: .5, delay: .1, repeat: -1, yoyo: true, ease: Power2.easeInOut });
            }});
        }});
        TweenMax.fromTo( promotionTitle.find('.live'), .5, { opacity: 0, x: -50 }, { opacity: 1, x: 0, delay: .8, ease: Back.easeOut.config(1) });
        TweenMax.fromTo( promotionTitle.find('.title-3'), .6, { opacity: 0, y: 50 }, { opacity: 1, y: 0, delay: .6, ease: Power2.easeOut });

        TweenMax.fromTo( $('.fix-right'), .6, { opacity: 0, y: 50 }, { opacity: 1, y: 0, delay: 1.4, ease: Sine.easeOut });

        TweenMax.fromTo( $('.sector-video'), .6, { opacity: 0, y: 20 }, { opacity: 1, y: 0, delay: 1.0, ease: Sine.easeOut });
    }

    function slideEvent(){
        $('.review-list').slick({
            autoplay: true,
            autoplaySpeed: 3000,
            speed: 1000,
            infinite: true,
            variableWidth: true,
            slidesToShow: 3
        });

        $('.review-list').on('init reInit afterChange', function (event, slick, currentSlide, nextSlide){
            //currentSlide is undefined on init -- set it to 0 in this case (currentSlide is 0 based)
            var i = (currentSlide ? currentSlide : 0) + 1;
            $('.review-list-info').text(i + '/' + slick.slideCount);
        });
    }

    function scrollEvent(){
        $(window).on('scroll', function(){
            var wt = $(window).scrollTop();
            var section = $('[class^="sector-"]');

            section.each(function(i){
                if( i == 4 ){
                    if( wt >= $('.sector-reason').offset().top ){
                        $('.sector-reason').addClass('active');
                    }
                } else {
                    if( wt >= section.eq(i).offset().top - (section.height()/2) ){
                        $(this).addClass('active');
                    } else {
                        //$(this).removeClass('active');
                    }
                }
            });
        });
    }
</script>
