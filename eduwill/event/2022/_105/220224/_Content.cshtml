<script type="text/javascript">
	var isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? true : false;
	var agent = navigator.userAgent.toLowerCase();
	var trident = navigator.userAgent.match(/Trident\/(\d.\d)/i);
	var url = window.location.href;
	var str = 'event.eduwill.net';
	var checkUrl = url.indexOf(str);

	$(function(){
		init();
	});

	function init(){
		scrollEvent();
		titleEvent();
		addEvent();
		slideEvent();
	};

	function addEvent(){
		pageMove('.page-move');
		/*if ("@(ViewBag.platformCd)" == "M") {
		}*/

	}
	

	function scrollEvent(){
		$(window).scroll(function () {
			var wt = $(window).scrollTop();
			var section = $('[class^="sector-"]');

			section.each(function(i){
				if( wt >= section.eq(i).offset().top - (section.height()/2) ){
					$(this).addClass('active');
				} else {
					$(this).removeClass('active');
				}
			});
		});
	}
	
	function titleEvent(){
		var $title = $('.sector-title');
		setTimeout(function() { $(".title-1").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 100);
		setTimeout(function() { $(".title-2").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 200);
		setTimeout(function() { $(".title-3").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 300);
		setTimeout(function() { $(".title-4").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 400);
		setTimeout(function() { $(".zero").css('opacity', '1').addClass('animate__flipInY animate__animated animate__faster') }, 600);
		setTimeout(function() { $(".won").css('opacity', '1').addClass('animate__fadeInLeft animate__animated animate__faster') }, 700);
		setTimeout(function() { $(".title-box").css('opacity', '1').addClass('animate__fadeIn animate__animated') }, 500);
		setTimeout(function() { $(".title-action").css('opacity', '1').addClass('animate__fadeInUp animate__animated animate__faster') }, 700);
	}

	
	function slideEvent(){
		$('.slider').each(function(key, item) {
			var sliderIdName = 'slider' + key;
				this.id = sliderIdName;
			var sliderId = '#' + sliderIdName;

			$(sliderId).slick({
				autoplay: true,
				autoplaySpeed: 2000,
				speed: 800,
				infinite: true,
				arrows: false,
				dots: true
			});
		});

		if( !isMobile && checkUrl ){
			_arrow = false;
			_dot = true;
			_boolean = true;

		} else {
			_arrow = false;
			_dot = true;
			_boolean = false;
		}

		$('.network-slider').slick({
			autoplay: true,
			autoplaySpeed: 2000,
			infinite: true,
			arrows: _arrow,
			dots:_dot,
			slidesToScroll: 1,
			speed: 800,
			centerMode: true,
			variableWidth: _boolean,
			responsive: [{
				breakpoint: 720,
				settings: {
					slidesToShow: 1,
				}
			}]
		});
	}
</script>