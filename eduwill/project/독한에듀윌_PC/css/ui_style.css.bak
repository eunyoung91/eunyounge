/*.sub #header h1 .fb { display:none;}*/
#header { position:absolute; top:0; left:0; width:100%; z-index:1000;}
#header h1 { position:absolute; top:60px; left:0; color:#fff; font-size:28px; font-weight:600; vertical-align:top;}
#header h1 a { color:#fff;}
#header h1 .fb { position:relative; top:7px; margin-left:4px; vertical-align:top;}
#header .toplink { position:absolute; top:15px; right:0; width:420px; overflow:hidden; text-align:right;}
#header .toplink li { display:inline-block; margin-left:20px;}
#header .toplink li:first-child { margin-left:0;}
#header .toplink li a { color:#fff; opacity:0.3; fillter:alpha(opacity=30);}
#header .toplink li a:hover { opacity:1; fillter:alpha(opacity=100);}
#header .toplink li a.btn_member { display:inline-block; padding:1px 8px 2px; border:1px solid #fff;}
#header .toplink li a.btn_member:hover { color:#cf2323; border-color:#cf2323;}
#header #gnb { position:absolute; right:0; top:70px; width:650px;}
#header #gnb > ul > li { position:relative; float:left; width:95px; margin-right:40px;}
#header #gnb > ul > li > a { position:relative; display:block; padding-bottom:10px; color:#fff; font-size:17px; font-weight:500;}
#header #gnb > ul > li > a .gnb_bar { display:block; position:absolute; bottom:0; width:100%; height:1px; }
#header #gnb > ul > li > a .gnb_bar:before { display:block; position:absolute; left:50%;width:0; height:1px; content:""; background:#f02222; -webkit-transform:translateX(-50%); -moz-transform:translateX(-50%); -ms-transform:translateX(-50%); -o-transform:translateX(-50%); transform:translateX(-50%); transition:all 0.4s ease;}
#header #gnb > ul > li:hover > a .gnb_bar:before { width:100%; -webkit-transition:all 0.4s ease;-moz-transition:all 0.4s ease;-o-transition:all 0.4s ease;-ms-transition:all 0.4s ease;transition:all 0.4s ease;}
#header #gnb > ul > li.depth03 { width:110px;}
#header #gnb > ul > li.depth05 { margin-right:0;}
#header #gnb > ul > li > .snb { display:none; position:relative; margin-top:10px; width:100%;}
#header #gnb > ul > li > .snb li a { text-align:center; color:#888; line-height:32px; font-weight:500;}
#header #gnb > ul > li > .snb li a:hover { color:#f02222;}
#header .bgBlack { position:absolute; top:0; left:0; width:100%; height:265px; background:url('//img.eduwill.net/resources/dokhan/dev/web/image/black.png') 0 0 repeat; z-index:-1; -webkit-transform:translateY(-200%);-moz-transform:translateY(-200%);transform:translateY(-100%);-webkit-transition:all 0.5s ease-in-out;-moz-transition:all 0.5s ease-in-out;transition:all 0.5s ease-in-out;}
#header.on .bgBlack { -webkit-transform:translateY(0px); -moz-transform:translateY(0px); transform:translateY(0px); -webkit-transition:all 0.5s ease-in-out; -moz-transition:all 0.5s ease-in-out; transition:all 0.5s ease-in-out;}
#header.on #gnb > ul > li > .snb { display:block;}

#visual { position:relative; width:100%; height:804px; background:#2d2c2c;}
.visual_con li { height:804px;}
.visual_con li > div { position:relative; width:1200px; height:100%; margin:0 auto;}
.visual_con li.scon01 { background:url('//img.eduwill.net/resources/dokhan/dev/web/image/bg_visual01.jpg') 50% 0 no-repeat;}
.visual_con li.scon02 { background:url('//img.eduwill.net/resources/dokhan/dev/web/image/bg_visual02.jpg') 50% 0 no-repeat;}
.visual_con li .m_title01 { position:absolute; top:290px; left:50%; width:496px; margin-left:-248px; color:#fff; font-size:24px; font-weight:400; letter-spacing:-1px;}
