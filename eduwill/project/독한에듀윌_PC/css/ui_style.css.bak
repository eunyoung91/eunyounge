.visual_con .bx-wrapper .bx-pager.bx-default-pager a.active { background:#cf2323;}
.visual_con .m_btn_set { position:absolute; top:585px; left:50%; width:952px; margin-left:-476px; }
.visual_con .m_btn_set dl { float:left; width:512px; margin-left:48px; text-align:left; /*-ms-transform: rotate(90deg); -webkit-transform: rotate(90deg); transform: rotate(90deg);*/}
