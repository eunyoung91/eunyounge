.timeOff_icon { position:absolute; top:-14px; right:76px; padding-bottom:9px; z-index:10;}
.timeOff_icon span.ico_box { display:block; padding:6px 15px; font-size:18px; font-weight:500; color:#fff; background:#e91b00;}
.timeOff_icon span.arrow { display:block; position:absolute; bottom:0; left:50%; margin-left:-6px; width:12px; height:9px; background:url('//img.eduwill.net/resources/dokhan/dev/web/image/ico_arrow02.png') 0 0 no-repeat;}

.explain_con { position:relative; margin:40px 0 0; overflow:hidden; padding-bottom:90px; min-height:245px; border-bottom:1px dotted #ccc;}
.explain_con.onlyexp .answer { display:none;}
.explain_con.onlyexp .explain_box { width:1016px;}
.explain_con .answer { float:left; padding-top:30px; width:204px; text-align:center;}
.explain_con .answer p { margin-top:20px; color:#222; font-size:18px; line-height:28px;}
.explain_con .answer p strong { font-size:26px; font-weight:700;}
.explain_con .explain_box { float:left; width:812px; padding:26px 42px; min-height:245px; background:url('//img.eduwill.net/resources/dokhan/dev/web/image/bu_box.gif') 0 0 repeat-y;}
.explain_con .explain_box .answer_text { margin-bottom:20px; font-size:22px; color:#000; font-weight:700;}
.explain_con .explain_box dl dt { margin-bottom:20px;}
.explain_con .explain_box dl dt span { display:inline-block; padding-bottom:3px; font-size:16px; font-weight:500; color:#222; border-bottom:2px solid #222;}
.explain_con .explain_box dl dd { color:#666; font-size:14px; line-height:26px;}
.quiz_con .bbs_wrap { margin-top:40px;}

/* bbs style */
.bbs_wrap { position:relative;}
.bbs_header { position:relative; margin-bottom:20px;}
.bbs_header .tab_list { padding-left:38px;}

.bbs_list { }
.bbs_list .select { position:absolute; top:0; right:0; width:170px; z-index:10;}
.bbs_list table { width:100%; border-top:1px solid #bfbfbf; border-bottom:1px solid #bfbfbf;}
.bbs_list table th { height:38px; vertical-align:middle; font-size:14px; color:#000; font-weight:500; text-align:center; border-bottom:1px solid #bfbfbf;}
.bbs_list table td { height:55px; text-align:center; vertical-align:middle; color:#999; font-size:14px; font-weight:500; border-top:1px solid #eee;}
.bbs_list table tr:first-child td { border-top:0 none;}
.bbs_list table td.num { color:#999; font-size:12px; font-weight:400;}
.bbs_list table td.subject { font-size:14px; color:#000; font-weight:500;}
.bbs_list table td.title { padding-left:20px; text-align:left;}
.bbs_list table td.title a { display:inline-block; width:500px; text-overflow:ellipsis; overflow:hidden; white-space:nowrap; word-break:all; color:#666; font-size:14px; font-weight:400;}
.bbs_list table td.date { color:#999; font-weight:400; font-size:12px;}
.bbs_list table td.myAns { color:#000; font-weight:500;}

.new { vertical-align:middle; margin-left:3px;}

.bbs_form {}
.bbs_form dl dt { padding-left:3px; margin-bottom:9px; font-weight:500; color:#666; font-size:14px;}
.bbs_form dl dd { margin-bottom:15px;}
.bbs_form .btn_position_r { text-align:right;}

.bbs_list.qna_style table tr.cont { display:none;}
.bbs_list.qna_style table tr.cont td { padding:0;}
.bbs_list.qna_style table tr.cont td .inquiry { position:relative; padding:18px 40px 65px 130px; text-align:left; background:#fff;}
.bbs_list.qna_style table tr.cont td .inquiry .inqcon { width:95%; color:#666; font-weight:400; font-size:14px; line-height:1.5;}
.bbs_list.qna_style table tr.cont td .inquiry .inqinput { margin-top:20px;}
.bbs_list.qna_style table tr.cont td .inquiry .replycon { margin-top:25px; padding-left:18px; border-left:2px solid #686868;}
.bbs_list.qna_style table tr.cont td .inquiry .replycon dl dt { margin-bottom:5px; font-weight:500; font-size:14px; color:#000; line-height:1.5;}
.bbs_list.qna_style table tr.cont td .inquiry .replycon dl dd { font-weight:400; font-size:14px; color:#000; line-height:1.5;}
.bbs_list.qna_style table tr.cont td .inquiry .replycon dl dd.from_user { color:#999;}
.bbs_list.qna_style table tr.cont td .inquiry .close_con { position:absolute; top:14px; right:36px;}
.bbs_list.qna_style table tr.cont td .inquiry .btn_position_r { position:absolute; right:36px; bottom:20px; text-align:right;}

.paging { position:relative; margin:25px 0; text-align:center;}
.paging a span { display:inline-block; margin:0 16px; padding:0 2px 1px 2px; font-size:14px; color:#999; font-weight:500; border-bottom:2px solid transparent;}
.paging a span.current { color:#000; border-color:#000; font-weight:700;}
.paging a.current span { color:#000; border-color:#000; font-weight:700;}

.control_calendar { margin-bottom:20px; text-align:center;}
.control_calendar a { display:inline-block;}
.control_calendar a img { display:inline-block;}
.control_calendar .prev, .control_calendar .next { width:21px; height:17px; border:1px solid #e2e2e2; vertical-align:middle;}
.control_calendar .caltxt { margin:0 8px;}
.control_calendar .caltxt input { display:inline; width:115px; border:0 none; background:none; cursor:pointer; text-align:center; font-size:30px; font-weight:500; color:#222; vertical-align:middle;}

.month_history .study_days { margin-bottom:20px;}
.month_history .days .d_title { font-size:18px;}
.month_history .days.box01 .d_title, .month_history .days.box01 .pct { color:#ff3e25;}
.month_history .days.box02 .d_title, .month_history .days.box02 .pct { color:#5194e1;}
.month_history .days .pct { margin-bottom:6px; font-size:32px; font-weight:500;}
.month_history .days .start, .month_history .days .end { font-family:돋움, Dotum; font-size:11px; line-height:1.5;}
.month_history .days div span { font-weight:bold !important;}

.clendar {}
.clendar table { border-top:1px solid #e0e2ea; border-right:1px solid #e0e2ea;}
.clendar table thead th { height:60px; text-align:center; vertical-align:middle; font-size:11px; font-family:맑은 고딕, Malgun gothic; color:#9ea4b8; font-weight:normal; border-bottom:1px solid #e0e2ea;}
.clendar table thead th:first-child { border-left:1px solid #e0e2ea;}
.clendar table tbody td { padding:0; margin:0; height:134px; border-left:1px solid #e0e2ea; border-bottom:1px solid #e0e2ea;}
.clendar table tbody td .cols { padding:18px; height:99px;}
.clendar table tbody td .cols .date { margin-bottom:12px; font-size:11px; font-family:맑은 고딕, Malgun gothic; color:#666;}
.clendar table tbody td .cols ul li { margin-top:6px; padding-left:30px; height:16px; font-size:11px; font-family:돋움, Dotum; color:#666; letter-spacing:-1px; line-height:16px;}
.clendar table tbody td .cols ul li.c1 { background:url('//img.eduwill.net/resources/dokhan/dev/web/image/ico_c1.gif') 0 0 no-repeat;}
.clendar table tbody td .cols ul li.c2 { background:url('//img.eduwill.net/resources/dokhan/dev/web/image/ico_c2.gif') 0 0 no-repeat;}
.clendar table tbody td .cols ul li.c3 { background:url('//img.eduwill.net/resources/dokhan/dev/web/image/ico_c3.gif') 0 0 no-repeat;}
.clendar table tbody td .cols.before .date { color:#aeafb4;}
.clendar table tbody td .cols.today { background:#f5f5f5;}