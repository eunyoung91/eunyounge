.visual_con .m_btn_set a.btn_apply { position:relative; display:block; float:right; width:392px; height:74px;}
.visual_con .m_btn_set a.btn_apply span { position:relative; display:block; padding:0 0 0 42px; height:70px; text-align:left; font-size:21px; color:#fff; line-height:70px; letter-spacing:-1px; font-weight:500; border:2px solid #fff; background:url('//img.eduwill.net/resources/dokhan/dev/web/image/bu_arrow07.png') 291px 30px no-repeat; z-index:2;}
.visual_con .m_btn_set a.btn_apply .square{ display:block; position:absolute; bottom:0; left:0; width:100%; height:0; z-index:1; background:#383737;}

